{
  "name": "TicTacToe",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rimraf dist/*",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "dist": "npm run clean && npm run copy & webpack --config webpack.dist.config.js",
    "ios": "react-native bundle --entry-file src/index.ios.js --bundle-output ios/main.jsbundle --platform ios --dev false",
    "android": "react-native bundle --entry-file src/index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --platform android --dev false",
    "lint": "eslint ./src",
    "serve": "node server.js",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "babel-eslint": "^7.1.1",
    "react": "15.4.1",
    "react-dom": "^15.0.0",
    "react-native": "0.39.2",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.7.2",
    "babel-jest": "18.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-native": "^1.9.1",
    "babel-register": "^6.7.2",
    "css-loader": "^0.23.1",
    "eslint": "^3.9.1",
    "eslint-plugin-react": "^4.2.3",
    "file-loader": "^0.8.5",
    "jasmine-core": "^2.4.1",
    "jest": "18.0.0",
    "jshint": "^2.9.1",
    "jshint-loader": "^0.8.3",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-script-launcher": "^0.2.0",
    "karma-webpack": "^1.7.0",
    "node-sass": "^3.4.2",
    "phantomjs-prebuilt": "^2.1.5",
    "react-hot-loader": "^1.3.0",
    "react-test-renderer": "15.4.1",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverage": true
  }
}
